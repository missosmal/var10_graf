<UserControl x:Class="var10_graf.Elements.PcItm"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:var10_graf.Elements"
             mc:Ignorable="d" >
    <Grid Background="LightPink" Margin="10">
        <StackPanel>
            <Label x:Name="Data" Content="Дата:..." HorizontalAlignment="Left" VerticalAlignment="Center"/>
            <Label x:Name="Time" Content="Время аренда:..." HorizontalAlignment="Left" VerticalAlignment="Center"/>
            <Label x:Name="FIO" Content="ФИО:..." HorizontalAlignment="Left" VerticalAlignment="Center"/>
            <Label x:Name="phone_number" Content="Номер телефона:..." HorizontalAlignment="Left" VerticalAlignment="Center"/>
            <Label x:Name="passport" Content="Паспортные данные:..." HorizontalAlignment="Left" VerticalAlignment="Center"/>

            <Button Content="Изменить" Click="Edit_button" HorizontalAlignment="Left" VerticalAlignment="Center" Width="100" Height="30" Background="WhiteSmoke" Margin="5"/>
            <Button Content="Удалить" Click="Delete_button" HorizontalAlignment="Left" VerticalAlignment="Center" Width="100" Height="30" Background="WhiteSmoke" Margin="5"/>
        </StackPanel>
    </Grid>
</UserControl>
